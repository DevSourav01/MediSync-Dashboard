{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ESNext",
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "moduleResolution": "Node",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "esModuleInterop": true,
"allowSyntheticDefaultImports": true,

  },
  "include": ["src"]
}
